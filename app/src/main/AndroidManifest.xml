            android:label="@string/title_activity_examp_lengeth"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.Alnajim.osama.StarLanguage.QuestionGame.addQuestionLanguage"
            android:label="@string/title_activity_add_question_language"
